{
  "manifest_version": 3,
  "name": "SecureLink Analyzer",
  "version": "1.0.0",
  "description": "AI-powered URL safety analyzer with real-time risk scoring and detailed explanations",
  "icons": {
    "16": "icons/default16.png",
    "48": "icons/default48.png",
    "128": "icons/default128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/default16.png",
      "48": "icons/default48.png",
      "128": "icons/default128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "content.js"
      ],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "notifications",
    "webNavigation"
  ],
  "host_permissions": [
    "https://5391fa77-5abb-4491-8f37-27eeaa781524-00-rm8ys4z76cle.pike.replit.dev/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "warningCaution.html",
        "warningDanger.html",
        "warningCaution.js",
        "warningDanger.js",
        "icons/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}